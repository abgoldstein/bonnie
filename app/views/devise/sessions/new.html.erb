<%= form_for(resource, :as => resource_name, :url => session_path(resource_name)) do |f| %>
<div id="login_dialog">
  <div id="cap-top"></div>
  <div id="cap-body">
    <div id="branding">
      <%= image_tag "bonnie_welcome.png", :id => 'imgLogo', :title => 'Bonnie welcomes you!', :alt => 'bonnie logo' %>
    </div>
    <div class="panel">
      <%= f.label :username %>
      <%= f.email_field :username %>
      <%= f.label :password %>
      <%= f.password_field :password, :onkeypress => "return submitKeyPress(event);" %>
      
      <div class="accountError">
        <%= alert %>
      </div>

      <div class="actions">
        <a class="btn" href="#" onclick="submitLoginform();"><span>Login</span></a>
      </div>

      <%= render :partial => "devise/shared/links" %>

    </div>
  </div>
  <div id="cap-bottom"></div>
</div><!-- END #login -->
<% end %>

